{
  "name": "@isignif/pdf-ai",
  "private": "true",
  "type": "module",
  "scripts": {
    "dev": "node --experimental-strip-types --watch src/index.ts",
    "build": "tsc",
    "start": "node --experimental-strip-types src/index.ts",
    "generate-client": "swagger-typescript-api generate -p https://isignif.fr/openapi.yml --js -o src/lib/isignif/",
    "generate-client:local": "swagger-typescript-api generate -p http://localhost:3000/openapi.yml --js -o src/lib/isignif/"
  },
  "dependencies": {
    "@hono/node-server": "^1.14.1",
    "@hono/zod-validator": "^0.5.0",
    "@mistralai/mistralai": "^1.6.0",
    "hono": "^4.7.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "swagger-typescript-api": "^13.1.3",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3"
  }
}
